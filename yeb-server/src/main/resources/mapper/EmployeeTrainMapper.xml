        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhangyu.server.mapper.EmployeeTrainMapper">

    <resultMap id="BaseResultMap" type="com.zhangyu.server.pojo.EmployeeTrain">
